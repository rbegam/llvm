BOOST_ROOT=/usr/intel/pkgs/boost/1.59.0/
all: ROIhandler
ROIhandler: ROIhandler.C fpgaROImarkerhandler.C
	csa-clang -target x86_64-unknown-linux-gnu -g -emit-llvm ROIhandler.C -c -o ROIhandler.bc
	csa-clang -target x86_64-unknown-linux-gnu -g -emit-llvm fpgaROImarkerhandler.C -c -o fpgaROImarkerhandler.bc
clean: 
	rm *.bc *.o
